<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
	xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:prism="http://prismlibrary.com"
    prism:ViewModelLocator.AutowireViewModel="True"
	xmlns:local="clr-namespace:CourtCheckInPrism.Views;assembly=CourtCheckInPrism"
    x:Class="CourtCheckInPrism.Views.AddVisit"
    Title="Add Visit">



	  <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
          <Label Text="AddVisit" VerticalOptions="Center" HorizontalOptions="Center" />
		          <Button Text="Visit Schedule" Command="{Binding Path=NavigateAbsoluteCommand}" CommandParameter="/VisitSchedule" />
          <Button Text="Check In" Command="{Binding Path=NavigateCommand}" CommandParameter="CheckIn" />
	  </StackLayout>
    

</ContentPage>

